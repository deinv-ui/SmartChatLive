{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "moduleResolution":"nodenext",
    "allowJs": true,
    "checkJs": false,
    "target": "esnext",
    "module": "NodeNext",
    "jsx": "react-jsx",
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "lib": ["DOM", "DOM.Iterable", "ESNext"]
  },
  "include": ["src"]
}
